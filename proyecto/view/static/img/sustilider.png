    <script>
      class MenuBurger {
        constructor() {
          this.element = document.querySelector(".js-menu-mobile");
          this.parentBtn = this.element.closest(".js-target-menu-mobile");
          this.closetBtn = document.querySelector(".js-menu-close");
          this.navMenu = this.element.nextElementSibling;
          this.classHidde = "is-hidde";
          this.init();
        }

        init() {
          this.showMenuMobile();
          this.hideMenuMobile();
        }

        showMenuMobile() {
          this.element.addEventListener("click", () => {
            this.navMenu.classList.remove(this.classHidde);
            document.querySelector("body").style.overflow = "hidden";
            this.parentBtn.classList.add("is-overflow-visible");
          });
        }

        hideMenuMobile() {
          this.closetBtn.addEventListener("click", () => {
            this.navMenu.classList.add(this.classHidde);
          });
        }
      }

      new MenuBurger();
    </script>		</div>
